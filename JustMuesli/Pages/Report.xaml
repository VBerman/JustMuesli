<Page x:Class="JustMuesli.Pages.Report"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:JustMuesli.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Report">

    <Grid>
        <UniformGrid Columns="2">
            <Frame Name="Order" IsEnabled="False"/>
            <Frame Name="ContactInfo" IsEnabled="False"/>
            
        </UniformGrid>
        <Button Content="Back to menu" Click="BackButtonClick" Name="BackToMenuButton" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
    </Grid>
</Page>
